{"ast":null,"code":"import { MyAction } from \"../actions/actions\";\nexport const myListReducer = (state = [], action) => {\n  switch (action.type) {\n    case MyAction.ADDTOMYLIST:\n      return [...state, action.payload];\n\n    case MyAction.REMOVEFROMLIST:\n      return [...state.filter(items => items.id != action.payload)];\n\n    case MyAction.GETALLLIST:\n      return action.payload;\n\n    default:\n      return state;\n  }\n};","map":{"version":3,"sources":["/workspaces/anime_website/redux/reducers/myListReducer.js"],"names":["MyAction","myListReducer","state","action","type","ADDTOMYLIST","payload","REMOVEFROMLIST","filter","items","id","GETALLLIST"],"mappings":"AAAA,SAASA,QAAT,QAAyB,oBAAzB;AAEA,OAAO,MAAMC,aAAa,GAAG,CAACC,KAAK,GAAG,EAAT,EAAaC,MAAb,KAAwB;AACnD,UAAQA,MAAM,CAACC,IAAf;AACE,SAAKJ,QAAQ,CAACK,WAAd;AACE,aAAO,CAAC,GAAGH,KAAJ,EAAWC,MAAM,CAACG,OAAlB,CAAP;;AACF,SAAKN,QAAQ,CAACO,cAAd;AACE,aAAO,CAAC,GAAGL,KAAK,CAACM,MAAN,CAAcC,KAAD,IAAWA,KAAK,CAACC,EAAN,IAAYP,MAAM,CAACG,OAA3C,CAAJ,CAAP;;AACF,SAAKN,QAAQ,CAACW,UAAd;AACE,aAAOR,MAAM,CAACG,OAAd;;AACF;AACE,aAAOJ,KAAP;AARJ;AAUD,CAXM","sourcesContent":["import { MyAction } from \"../actions/actions\";\n\nexport const myListReducer = (state = [], action) => {\n  switch (action.type) {\n    case MyAction.ADDTOMYLIST:\n      return [...state, action.payload];\n    case MyAction.REMOVEFROMLIST:\n      return [...state.filter((items) => items.id != action.payload)];\n    case MyAction.GETALLLIST:\n      return action.payload;\n    default:\n      return state;\n  }\n};\n"]},"metadata":{},"sourceType":"module"}